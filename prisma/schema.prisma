datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Occurrence {
  id          Int      @id @default(autoincrement())
  plate       String
  cameraName  String
  latitude    Float
  longitude   Float
  imageBase64 String
  status      String
  createdAt   DateTime @default(now())
}
